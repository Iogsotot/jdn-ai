{
  "env": {
    "browser": true,
    "es6": true,
    "webextensions": true,
    "amd": true,
    "node": true
  },
  "extends": ["plugin:react/recommended", "google", "plugin:@typescript-eslint/recommended"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly",
    "chrome": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "plugins": ["react", "@typescript-eslint"],
  "rules": {
    "ban-ts-comment": 0,
    "quotes": 0,
    "require-jsdoc": 0,
    "camelcase": 0,
    "max-len": [1, { "code": 120 }],
    "object-curly-spacing": 0,
    "comma-dangle": 0,
    "react/prop-types": [
      0
    ]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
